<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="#FFFFFF" />
            <corners android:radius="100dp" />
            <padding
                android:left="5dp"
                android:top="5dp"
                android:right="5dp"
                android:bottom="5dp" />
        </shape>
    </item>

    <item>
        <shape android:shape="oval">
            <gradient
                android:startColor="#F1DCE8"
                android:endColor="#EDA1BB"
                android:angle="45" />
            <stroke
                android:width="4dp"
                android:color="#FB8B6F" />
            <corners android:radius="100dp" />
        </shape>
    </item>
</layer-list>
